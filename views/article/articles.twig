{% extends 'templates/default.twig' %}

{% block content %}
	<div class="container">
		<table class="table">
		  <thead class="thead-inverse">
		    <tr>
		      <th>First Name</th>
		      <th>Last Name</th>
		      <th>Username</th>
		    </tr>
		  </thead>
		  {% for item in article %}
		    <tr>
		      <td>{{ item.image }}</td>
		      <td>{{ item.title }}</td>
		      <td>{{ item.content }}</td>
		    </tr>
		   {% endfor %}
		</table>
	</div>
{% endblock  %}